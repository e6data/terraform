{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "ec2:AuthorizeSecurityGroupIngress",
                "ec2:AuthorizeSecurityGroupEgress",
                "ec2:DeleteSubnet",
                "ec2:DeleteVpcEndpoints",
                "eks:UpdateClusterConfig",
                "ec2:AttachInternetGateway",
                "ec2:AssociateVpcCidrBlock",
                "ec2:DeleteRouteTable",
                "ec2:AssociateRouteTable",
                "eks:DescribeNodegroup",
                "ec2:CreateRoute",
                "ec2:CreateInternetGateway",
                "ec2:RevokeSecurityGroupEgress",
                "autoscaling:UpdateAutoScalingGroup",
                "ec2:DeleteInternetGateway",
                "autoscaling:DeleteTags",
                "ec2:ModifyAddressAttribute",
                "ec2:CreateTags",
                "ec2:CreateRouteTable",
                "ec2:RunInstances",
                "ec2:DetachInternetGateway",
                "ec2:DisassociateRouteTable",
                "eks:DeleteCluster",
                "ec2:RevokeSecurityGroupIngress",
                "eks:DeleteNodegroup",
                "autoscaling:CreateOrUpdateTags",
                "eks:AccessKubernetesApi",
                "eks:UpdateNodegroupConfig",
                "ec2:DeleteNatGateway",
                "eks:DescribeCluster",
                "ec2:DeleteVpc",
                "ec2:CreateSubnet",
                "ec2:AssociateAddress",
                "ec2:ModifyVpcEndpoint",
                "ec2:DisassociateAddress",
                "eks:UpdateClusterVersion",
                "ec2:DeleteTags",
                "ec2:CreateNatGateway",
                "ec2:CreateVpc",
                "ec2:ModifySubnetAttribute",
                "eks:UpdateNodegroupVersion",
                "ec2:CreateSecurityGroup",
                "ec2:ModifyVpcAttribute",
                "ec2:ReleaseAddress",
                "ec2:DeleteLaunchTemplate",
                "ec2:ResetAddressAttribute",
                "eks:UntagResource",
                "ec2:DeleteRoute",
                "eks:CreateNodegroup",
                "ec2:AllocateAddress",
                "ec2:CreateLaunchTemplateVersion",
                "ec2:CreateLaunchTemplate",
                "eks:DescribeUpdate",
                "ec2:CreateVpcEndpoint",
                "eks:TagResource",
                "ec2:DeleteSecurityGroup",
                "ec2:ModifyLaunchTemplate",
                "eks:CreateAccessEntry",
                "eks:DeleteAccessEntry",
                "eks:TagResource",
                "eks:UntagResource",
                "eks:DescribeAccessEntry",
                "eks:AssociateAccessPolicy",
                "eks:ListAssociatedAccessPolicies",
                "eks:DisassociateAccessPolicy",
                "ec2:CreateNetworkAcl",
                "ec2:CreateNetworkAclEntry",
                "ec2:DeleteNetworkAcl",
                "ec2:DeleteNetworkAclEntry"
            ],
            "Resource": [
                "arn:aws:eks:<AWS_REGION>:<AWS_ACCOUNT_ID>:nodegroup/e6data-*",
                "arn:aws:eks:<AWS_REGION>:<AWS_ACCOUNT_ID>:cluster/e6data-*",
                "arn:aws:autoscaling:<AWS_REGION>:<AWS_ACCOUNT_ID>:autoScalingGroup:*:autoScalingGroupName/eks-*",
                "arn:aws:ec2:<AWS_REGION>:<AWS_ACCOUNT_ID>:vpc-endpoint/*",
                "arn:aws:ec2:<AWS_REGION>:<AWS_ACCOUNT_ID>:elastic-ip/*",
                "arn:aws:ec2:<AWS_REGION>:<AWS_ACCOUNT_ID>:vpc/*",
                "arn:aws:ec2:<AWS_REGION>:<AWS_ACCOUNT_ID>:launch-template/*",
                "arn:aws:ec2:<AWS_REGION>:<AWS_ACCOUNT_ID>:instance/*",
                "arn:aws:ec2:<AWS_REGION>:<AWS_ACCOUNT_ID>:natgateway/*",
                "arn:aws:ec2:<AWS_REGION>:<AWS_ACCOUNT_ID>:subnet/*",
                "arn:aws:ec2:<AWS_REGION>::image/*",
                "arn:aws:ec2:<AWS_REGION>:<AWS_ACCOUNT_ID>:route-table/*",
                "arn:aws:ec2:<AWS_REGION>:<AWS_ACCOUNT_ID>:internet-gateway/*",
                "arn:aws:ec2:<AWS_REGION>:<AWS_ACCOUNT_ID>:volume/*",
                "arn:aws:ec2:<AWS_REGION>:<AWS_ACCOUNT_ID>:security-group/*",
                "arn:aws:ec2:<AWS_REGION>:<AWS_ACCOUNT_ID>:network-interface/*",
                "arn:aws:eks:<AWS_REGION>:<AWS_ACCOUNT_ID>:access-entry/e6data-*/*",
                "arn:aws:ec2:<AWS_REGION>:<AWS_ACCOUNT_ID>:network-acl/*"
            ]
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": [
                "ec2:DescribeAddresses",
                "iam:ListRoleTags",
                "iam:UntagRole",
                "ec2:MoveAddressToVpc",
                "iam:TagRole",
                "events:PutRule",
                "events:TagResource",
                "ec2:DescribeRegions",
                "iam:ListInstanceProfileTags",
                "sqs:tagqueue",
                "ec2:DescribeAddressesAttribute",
                "ec2:DescribeVpcAttribute",
                "ec2:DescribeInternetGateways",
                "ec2:GetLaunchTemplateData",
                "ec2:DescribeNetworkInterfaces",
                "ec2:DescribeAvailabilityZones",
                "ec2:DescribeAccountAttributes",
                "iam:ListPolicyTags",
                "ec2:DescribeRouteTables",
                "iam:GetRole",
                "iam:GetPolicy",
                "ec2:DescribeLaunchTemplates",
                "ec2:DescribeTags",
                "ec2:DescribeVpcClassicLinkDnsSupport",
                "ec2:DescribeLaunchTemplateVersions",
                "autoscaling:DescribeTags",
                "iam:ListRoles",
                "ec2:DescribeNatGateways",
                "iam:TagPolicy",
                "ec2:DescribePrefixLists",
                "ec2:DescribeSecurityGroups",
                "ec2:DescribeVpcClassicLink",
                "ec2:DescribeSecurityGroupRules",
                "ec2:DescribeVpcs",
                "iam:UntagPolicy",
                "iam:UntagOpenIDConnectProvider",
                "ec2:DescribeVpcEndpoints",
                "sqs:CreateQueue",
                "iam:UntagInstanceProfile",
                "ec2:DescribeSubnets",
                "iam:TagOpenIDConnectProvider",
                "iam:TagInstanceProfile",
                "sqs:getqueueattributes",
                "sqs:listqueuetags",
                "events:DescribeRule",
                "events:ListTagsForResource",
                "ec2:DisassociateAddress",
                "events:PutTargets",
                "events:ListTargetsByRule",
                "ec2:DescribeNetworkAcls"
            ],
            "Resource": "*"
        },
        {
            "Sid": "VisualEditor2",
            "Effect": "Allow",
            "Action": [
                "iam:UpdateAssumeRolePolicy",
                "iam:GetPolicyVersion",
                "iam:DeletePolicy",
                "iam:CreateRole",
                "iam:DeleteRole",
                "iam:AttachRolePolicy",
                "iam:CreateOpenIDConnectProvider",
                "iam:CreatePolicy",
                "iam:ListInstanceProfilesForRole",
                "iam:CreateServiceLinkedRole",
                "iam:DetachRolePolicy",
                "iam:ListPolicyVersions",
                "iam:ListAttachedRolePolicies",
                "iam:CreatePolicyVersion",
                "iam:ListRolePolicies",
                "iam:GetOpenIDConnectProvider",
                "iam:DeleteOpenIDConnectProvider",
                "iam:DeletePolicyVersion"
            ],
            "Resource": [
                "arn:aws:iam::<AWS_ACCOUNT_ID>:oidc-provider/*",
                "arn:aws:iam::<AWS_ACCOUNT_ID>:role/e6data-*",
                "arn:aws:iam::<AWS_ACCOUNT_ID>:role/aws-service-role/eks-nodegroup.amazonaws.com/AWSServiceRoleForAmazonEKSNodegroup",
                "arn:aws:iam::<AWS_ACCOUNT_ID>:policy/e6data-*"
            ]
        },
        {
            "Sid": "VisualEditor3",
            "Effect": "Allow",
            "Action": "iam:PassRole",
            "Resource": "arn:aws:iam::<AWS_ACCOUNT_ID>:role/e6data-*"
        },
        {
            "Sid": "VisualEditor4",
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": "arn:aws:s3:::e6data-*"
        },
        {
            "Sid": "VisualEditor5",
            "Effect": "Allow",
            "Action": "eks:CreateCluster",
            "Resource": [
                "arn:aws:eks:<AWS_REGION>:<AWS_ACCOUNT_ID>:nodegroup/e6data-*",
                "arn:aws:eks:<AWS_REGION>:<AWS_ACCOUNT_ID>:cluster/e6data-*"
            ]
        },
        {
            "Sid": "RulesManage",
            "Effect": "Allow",
            "Action": [
                "events:DeleteRule",
                "events:RemoveTargets"
            ],
            "Resource": [
                "arn:aws:events:<AWS_REGION>:<AWS_ACCOUNT_ID>:rule/e6data-*"
            ]
        },
        {
            "Sid": "SQSwrite",
            "Effect": "Allow",
            "Action": [
                "sqs:deletequeue",
                "sqs:setqueueattributes"
            ],
            "Resource": [
                "arn:aws:sqs:<AWS_REGION>:<AWS_ACCOUNT_ID>:e6data-*"
            ]
        },
        {
            "Sid": "Dissociateaddress",
            "Effect": "Allow",
            "Action": [
                "ec2:DisassociateAddress"
            ],
            "Resource": [
                "*"
            ],
            "Condition": {
                "StringLike": {
                    "aws:ResourceTag/Name": "e6data-*"
                }
            }
        },
        {
            "Sid": "CloudWatchLogGroup",
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogGroup",
                "logs:DescribeLogGroups",
                "logs:ListTagsLogGroup",
                "logs:PutRetentionPolicy",
                "logs:DeleteLogGroup",
                "logs:DeleteRetentionPolicy"
            ],
            "Resource": [
                "arn:aws:logs:<AWS_REGION>:<AWS_ACCOUNT_ID>:log-group::log-stream:*",
                "arn:aws:logs:<AWS_REGION>:<AWS_ACCOUNT_ID>:log-group:/aws/eks/e6data-*"
            ]
        }
    ]
}
